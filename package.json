{
  "name": "pseud-esp",
  "displayName": "Pseudocodigo Español",
  "description": "Sintaxis de pseudocodigo",
  "publisher": "fabior",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pseud-esp",
      "aliases": ["Pseudocodigo Español", "pseud-esp"],
      "extensions": [".pseud"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pseud-esp",
      "scopeName": "source.pseud",
      "path": "./syntaxes/pseud-esp.tmLanguage.json"
    }],
    "snippets": [{
      "language": "pseud-esp",
      "path": "./snippets/pseud-esp-snippets.json"
    },
    {
      "language": "plaintext",
      "path": "./snippets/text-snippets.json"
    }],
    "themes": [{
      "uiTheme": "vs-dark",
      "label": "pseud-colors",
      "path": "./themes/pseud-esp-theme.json"
    }]
  }
}
