{
  "name": "pseud-esp",
  "displayName": "Pseudocodigo Español",
  "description": "Sintaxis de pseudocodigo",
  "publisher": "fabior",
  "repository": {
    "type": "git",
    "url": "https://github.com/SymbiontZ/pseud-esp"
  },
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:pseud-esp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "pseud-esp",
        "aliases": [
          "Pseudocodigo Español",
          "pseud-esp"
        ],
        "extensions": [
          ".pseud"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "pseud-esp",
        "path": "./snippets/pseud-esp-snippets.json"
      },
      {
        "language": "plaintext",
        "path": "./snippets/text-snippets.json"
      }
    ]
  },
  "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p . --outDir out"
    },
    "devDependencies": {
        "typescript": "^5.3.3",
        "@types/vscode": "^1.92.0"
    }
}
